programa {
  funcao inicio() {
    
    // Declaração da variável
    cadeia mensagemUser

    // Entrada de dados, pergunta ao usuário
    escreva("Digite uma mensagem: ")    
    // Lê a mensagem digitada e guarda na variável
    leia(mensagemUser)

    // Mostra na tela a mensagem
    escreva("MSG: ", mensagemUser)

  }
}
